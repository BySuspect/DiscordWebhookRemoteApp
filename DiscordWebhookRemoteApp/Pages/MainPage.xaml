<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="DiscordWebhookRemoteApp.Pages.MainPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:adscontrol="clr-namespace:MarcTron.Plugin.Controls;assembly=Plugin.MtAdmob"
    xmlns:chbx="clr-namespace:IntelliAbb.Xamarin.Controls;assembly=IntelliAbb.Xamarin.Controls"
    xmlns:controls="clr-namespace:MarcTron.Plugin.Controls;assembly=Plugin.MtAdmob"
    xmlns:customItems="clr-namespace:DiscordWebhookRemoteApp.CustomItems"
    xmlns:customViews="clr-namespace:DiscordWebhookRemoteApp.CustomViews"
    xmlns:helpers="clr-namespace:DiscordWebhookRemoteApp.Helpers"
    xmlns:xct="http://xamarin.com/schemas/2020/toolkit"
    xct:StatusBarEffect.Color="{x:Static helpers:ThemeColors.StatusBarColor}"
    xct:StatusBarEffect.Style="{x:Static helpers:ThemeColors.StatusBarStyle}"
    BackgroundColor="{x:Static helpers:ThemeColors.BackColor}"
    BackgroundImageSource="{x:Static helpers:ThemeColors.backgroundImg}">

    <ContentPage.ToolbarItems>
        <ToolbarItem Clicked="FeedbackButton_Clicked" IconImageSource="feedback32px.png" />
        <ToolbarItem Clicked="DiscordButton_Clicked" IconImageSource="dcdemoimage.png" />
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <StackLayout Margin="5,5,5,0">
            <Grid HorizontalOptions="{OnIdiom Phone=Fill, Tablet=Center}" VerticalOptions="FillAndExpand">
                <ScrollView HorizontalScrollBarVisibility="Never" VerticalScrollBarVisibility="Never">
                    <StackLayout Padding="0,0,0,50" VerticalOptions="FillAndExpand">

                        <!--#region Webhooks Select Area-->
                        <Frame
                            Padding="5,10"
                            BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                            BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                            CornerRadius="15"
                            IsVisible="True">
                            <StackLayout>
                                <Label
                                    Margin="10,0,0,0"
                                    FontSize="Caption"
                                    HorizontalOptions="Start"
                                    Text="Select Webhook"
                                    TextColor="{x:Static helpers:ThemeColors.TextColor}" />
                                <ScrollView HorizontalScrollBarVisibility="Never" Orientation="Horizontal">
                                    <StackLayout Orientation="Horizontal">
                                        <StackLayout x:Name="blSavedWebhooks" Orientation="Horizontal">
                                            <!--<BindableLayout.ItemsSource>
                                                <x:Array Type="{x:Type helpers:webhookItems}">
                                                    <helpers:webhookItems ID="0" name="awhgfjsasg" />
                                                    <helpers:webhookItems ID="1" name="jhgfjsjawg" />
                                                    <helpers:webhookItems ID="2" name="mjhgkfm" />
                                                    <helpers:webhookItems ID="3" name="aöçmhgf" />
                                                    <helpers:webhookItems ID="5" name="dhdkdrsh" />
                                                </x:Array>
                                            </BindableLayout.ItemsSource>-->
                                            <BindableLayout.ItemTemplate>
                                                <DataTemplate>
                                                    <Frame
                                                        xct:TouchEffect.NativeAnimation="true"
                                                        xct:TouchEffect.NativeAnimationColor="#2EFFCCA3"
                                                        xct:TouchEffect.PressedAnimationDuration="500"
                                                        xct:TouchEffect.PressedOpacity="0.6"
                                                        xct:TouchEffect.PressedScale="0.98"
                                                        AutomationId="{Binding ID}"
                                                        BackgroundColor="Transparent"
                                                        BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                        CornerRadius="360"
                                                        HeightRequest="40"
                                                        WidthRequest="40">
                                                        <Label
                                                            Margin="-5"
                                                            FontSize="Caption"
                                                            HorizontalTextAlignment="Center"
                                                            TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                                            VerticalOptions="Center">
                                                            <Label.FormattedText>
                                                                <FormattedString>
                                                                    <Span Text="{Binding name}" />
                                                                </FormattedString>
                                                            </Label.FormattedText>
                                                        </Label>
                                                        <Frame.GestureRecognizers>
                                                            <TapGestureRecognizer Tapped="WebhookSelect_Tapped" />
                                                        </Frame.GestureRecognizers>
                                                    </Frame>
                                                </DataTemplate>
                                            </BindableLayout.ItemTemplate>
                                        </StackLayout>
                                        <StackLayout>
                                            <Frame
                                                xct:TouchEffect.NativeAnimation="true"
                                                xct:TouchEffect.NativeAnimationColor="#2EFFCCA3"
                                                xct:TouchEffect.PressedAnimationDuration="500"
                                                xct:TouchEffect.PressedOpacity="0.6"
                                                xct:TouchEffect.PressedScale="0.98"
                                                BackgroundColor="Transparent"
                                                BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                CornerRadius="360"
                                                HeightRequest="40"
                                                HorizontalOptions="CenterAndExpand"
                                                VerticalOptions="CenterAndExpand"
                                                WidthRequest="40">
                                                <Image
                                                    xct:IconTintColorEffect.TintColor="{x:Static helpers:ThemeColors.TextColor}"
                                                    Scale=".35"
                                                    Source="plus64px.png" />
                                                <Frame.GestureRecognizers>
                                                    <TapGestureRecognizer Tapped="WebhookAdd_Tapped" />
                                                </Frame.GestureRecognizers>
                                            </Frame>
                                        </StackLayout>
                                    </StackLayout>
                                </ScrollView>
                                <Grid>
                                    <Label
                                        Grid.Column="0"
                                        Margin="10,10,0,10"
                                        FontSize="Caption"
                                        HorizontalOptions="Start"
                                        TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                        VerticalOptions="Center">
                                        <Label.FormattedText>
                                            <FormattedString>
                                                <Span Text="Selected: " />
                                                <Span x:Name="lblSelectedUrl" Text="none" />
                                            </FormattedString>
                                        </Label.FormattedText>
                                    </Label>
                                    <StackLayout
                                        Grid.Column="1"
                                        Margin="0,5,10,5"
                                        HorizontalOptions="EndAndExpand"
                                        Orientation="Vertical">
                                        <Button
                                            Padding="10,5,10,5"
                                            xct:TouchEffect.NativeAnimation="true"
                                            xct:TouchEffect.NativeAnimationColor="#2EFFCCA3"
                                            xct:TouchEffect.PressedAnimationDuration="500"
                                            xct:TouchEffect.PressedOpacity="0.6"
                                            xct:TouchEffect.PressedScale="0.98"
                                            BackgroundColor="Transparent"
                                            BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                            BorderWidth="1"
                                            Clicked="webhookEditDelete_Clicked"
                                            CornerRadius="15"
                                            FontSize="Caption"
                                            HeightRequest="40"
                                            HorizontalOptions="EndAndExpand"
                                            Text="Edit &amp; Delete"
                                            TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                            VerticalOptions="Center" />
                                        <Button
                                            Padding="10,5,10,5"
                                            xct:TouchEffect.NativeAnimation="true"
                                            xct:TouchEffect.NativeAnimationColor="#2EFFCCA3"
                                            xct:TouchEffect.PressedAnimationDuration="500"
                                            xct:TouchEffect.PressedOpacity="0.6"
                                            xct:TouchEffect.PressedScale="0.98"
                                            BackgroundColor="Transparent"
                                            BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                            BorderWidth="1"
                                            Clicked="ExportWebhooks_Clicked"
                                            CornerRadius="15"
                                            FontSize="Caption"
                                            HeightRequest="40"
                                            HorizontalOptions="EndAndExpand"
                                            Text="Export Webhooks"
                                            TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                            VerticalOptions="Center" />
                                    </StackLayout>
                                </Grid>
                            </StackLayout>
                        </Frame>
                        <!--#endregion-->

                        <!--<customItems:CustomAdsView
                            x:Name="adsTop"
                            AdsId="ca-app-pub-3881259676793306/6856903418"
                            AutomationId="adsTop"
                            HeightRequest="{OnIdiom Phone=50,
                                                    Tablet=90}"
                            VerticalOptions="Center" />-->

                        <!--#region Webhook Profile Area-->
                        <Frame
                            Padding="5"
                            BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                            BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                            CornerRadius="15"
                            IsVisible="True">
                            <StackLayout>
                                <Label
                                    Margin="10,0,0,0"
                                    FontSize="Caption"
                                    HorizontalOptions="Start"
                                    Text="Webhook Profile"
                                    TextColor="{x:Static helpers:ThemeColors.TextColor}" />
                                <StackLayout HeightRequest="45" Orientation="Horizontal">
                                    <Image
                                        x:Name="imgWebhookImage"
                                        Margin="10,0,0,0"
                                        xct:CornerRadiusEffect.CornerRadius="360"
                                        HeightRequest="45"
                                        Source="dcdemoimage.png">
                                        <Image.GestureRecognizers>
                                            <TapGestureRecognizer Tapped="WebhookImage_Tapped" />
                                        </Image.GestureRecognizers>
                                    </Image>
                                    <customItems:BorderlessEntry
                                        x:Name="entryWebhookName"
                                        Margin="10,0,0,0"
                                        Completed="entryWebhookName_Completed"
                                        FontSize="Small"
                                        HorizontalOptions="FillAndExpand"
                                        MaxLength="80"
                                        Placeholder="Webhook Name"
                                        PlaceholderColor="{x:Static helpers:ThemeColors.TransparentTextColor}"
                                        TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                        Unfocused="entryWebhookName_Unfocused"
                                        VerticalOptions="Center" />
                                    <Button
                                        Margin="0,0,10,0"
                                        xct:TouchEffect.NativeAnimation="true"
                                        xct:TouchEffect.NativeAnimationColor="#2EFFCCA3"
                                        xct:TouchEffect.PressedAnimationDuration="500"
                                        xct:TouchEffect.PressedOpacity="0.6"
                                        xct:TouchEffect.PressedScale="0.98"
                                        BackgroundColor="Transparent"
                                        BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                        BorderWidth="1"
                                        Clicked="webhookSaveLoad_Clicked"
                                        CornerRadius="15"
                                        FontSize="Caption"
                                        HeightRequest="40"
                                        HorizontalOptions="EndAndExpand"
                                        Text="Save &amp; Load"
                                        TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                        VerticalOptions="Center" />
                                </StackLayout>
                            </StackLayout>
                        </Frame>
                        <!--#endregion-->

                        <!--#region Send Message Area-->
                        <Frame
                            Padding="5"
                            BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                            BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                            CornerRadius="15"
                            IsVisible="True">
                            <StackLayout>
                                <Label
                                    Margin="10,0,0,0"
                                    FontSize="Caption"
                                    HorizontalOptions="Start"
                                    Text="Message Content"
                                    TextColor="{x:Static helpers:ThemeColors.TextColor}" />
                                <Frame
                                    Padding="5"
                                    BackgroundColor="Transparent"
                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                    CornerRadius="15">
                                    <customItems:CustomExpander
                                        x:Name="expMessageContent"
                                        AnimationLength="450"
                                        CollapseAnimationEasing="SpringOut"
                                        ExpandAnimationEasing="SpringIn"
                                        HeaderText="Send Message">
                                        <customItems:CustomExpander.Content>
                                            <Grid>
                                                <customItems:CustomEditor
                                                    x:Name="entryContentMessage"
                                                    Margin="10,5,5,25"
                                                    FontSize="Small"
                                                    HeightRequest="200"
                                                    HorizontalOptions="FillAndExpand"
                                                    MaxLength="2000"
                                                    Placeholder="Message"
                                                    PlaceholderColor="{x:Static helpers:ThemeColors.TransparentTextColor}"
                                                    TextColor="{x:Static helpers:ThemeColors.TextColor}" />
                                                <Label
                                                    Margin="5"
                                                    Padding="0,0,10,0"
                                                    FontSize="Caption"
                                                    HorizontalOptions="EndAndExpand"
                                                    TextColor="{Binding Source={x:Reference entryContentMessage}, Path=SizeLabelTextColor}"
                                                    VerticalOptions="EndAndExpand">
                                                    <Label.FormattedText>
                                                        <FormattedString>
                                                            <Span Text="{Binding Source={x:Reference entryContentMessage}, Path=TextLenght}" />
                                                            <Span Text="/" />
                                                            <Span Text="{Binding Source={x:Reference entryContentMessage}, Path=MaxLength}" />
                                                        </FormattedString>
                                                    </Label.FormattedText>
                                                </Label>
                                            </Grid>
                                        </customItems:CustomExpander.Content>
                                    </customItems:CustomExpander>
                                </Frame>
                            </StackLayout>
                        </Frame>
                        <!--#endregion-->

                        <!--#region Send Embed Area-->
                        <Frame
                            Padding="5"
                            BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                            BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                            CornerRadius="15">
                            <StackLayout>
                                <Label
                                    Margin="10,0,0,0"
                                    FontSize="Caption"
                                    HorizontalOptions="Start"
                                    Text="Embed Content"
                                    TextColor="{x:Static helpers:ThemeColors.TextColor}" />
                                <Frame
                                    Padding="5"
                                    BackgroundColor="Transparent"
                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                    CornerRadius="15">
                                    <customItems:CustomExpander
                                        x:Name="expEmbedContent"
                                        AnimationLength="450"
                                        CollapseAnimationEasing="SpringOut"
                                        ExpandAnimationEasing="SpringIn"
                                        HeaderText="Send Embed"
                                        IsExpanded="False">
                                        <customItems:CustomExpander.Content>
                                            <StackLayout>
                                                <!--#region Author-->
                                                <Frame
                                                    Padding="5"
                                                    BackgroundColor="Transparent"
                                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                    CornerRadius="15">
                                                    <customItems:CustomExpander
                                                        x:Name="expEmbedAuthor"
                                                        AnimationLength="450"
                                                        CollapseAnimationEasing="SpringOut"
                                                        ExpandAnimationEasing="SpringIn"
                                                        HeaderText="Author"
                                                        IsExpanded="False">
                                                        <customItems:CustomExpander.Content>
                                                            <StackLayout Margin="0,10">
                                                                <Frame
                                                                    Padding="5"
                                                                    BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                                    CornerRadius="15"
                                                                    HeightRequest="45">
                                                                    <StackLayout HeightRequest="45" Orientation="Horizontal">
                                                                        <Image
                                                                            x:Name="ImgEmbedAuthor"
                                                                            Margin="10,0,0,0"
                                                                            xct:CornerRadiusEffect.CornerRadius="360"
                                                                            HeightRequest="45"
                                                                            IsAnimationPlaying="True"
                                                                            Source="dcdemoimage.png"
                                                                            WidthRequest="45">
                                                                            <Image.GestureRecognizers>
                                                                                <TapGestureRecognizer Tapped="EmbedAuthorImage_Tapped" />
                                                                            </Image.GestureRecognizers>
                                                                        </Image>
                                                                        <Grid HorizontalOptions="FillAndExpand">
                                                                            <customItems:CustomEntry
                                                                                x:Name="EntryEmbedAuthorName"
                                                                                Margin="10,0,10,0"
                                                                                FontSize="Small"
                                                                                MaxLength="256"
                                                                                Placeholder="Author"
                                                                                PlaceholderColor="{x:Static helpers:ThemeColors.TransparentTextColor}"
                                                                                TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                                                                VerticalOptions="Center" />
                                                                            <Label
                                                                                Padding="0,0,10,0"
                                                                                FontSize="Caption"
                                                                                HorizontalOptions="EndAndExpand"
                                                                                TextColor="{Binding Source={x:Reference EntryEmbedAuthorName}, Path=SizeLabelTextColor}"
                                                                                VerticalOptions="End">
                                                                                <Label.FormattedText>
                                                                                    <FormattedString>
                                                                                        <Span Text="{Binding Source={x:Reference EntryEmbedAuthorName}, Path=TextLenght}" />
                                                                                        <Span Text="/" />
                                                                                        <Span Text="{Binding Source={x:Reference EntryEmbedAuthorName}, Path=MaxLength}" />
                                                                                    </FormattedString>
                                                                                </Label.FormattedText>
                                                                            </Label>
                                                                        </Grid>
                                                                    </StackLayout>
                                                                </Frame>
                                                                <Frame
                                                                    Padding="5"
                                                                    BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                                    CornerRadius="15">
                                                                    <StackLayout>
                                                                        <customItems:BorderlessEntry
                                                                            x:Name="EntryEmbedAuthorUrl"
                                                                            Margin="10,0,0,0"
                                                                            FontSize="Small"
                                                                            HorizontalOptions="FillAndExpand"
                                                                            Placeholder="URL"
                                                                            PlaceholderColor="{x:Static helpers:ThemeColors.TransparentTextColor}"
                                                                            TextColor="{x:Static helpers:ThemeColors.TextColor}" />
                                                                    </StackLayout>
                                                                </Frame>
                                                            </StackLayout>
                                                        </customItems:CustomExpander.Content>
                                                    </customItems:CustomExpander>
                                                </Frame>
                                                <!--#endregion-->

                                                <!--#region Body-->
                                                <Frame
                                                    Padding="5"
                                                    BackgroundColor="Transparent"
                                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                    CornerRadius="15">
                                                    <customItems:CustomExpander
                                                        x:Name="expEmbedBody"
                                                        AnimationLength="450"
                                                        CollapseAnimationEasing="SpringOut"
                                                        ExpandAnimationEasing="SpringIn"
                                                        HeaderText="Body"
                                                        IsExpanded="False">
                                                        <customItems:CustomExpander.Content>
                                                            <StackLayout Margin="0,10">
                                                                <Frame
                                                                    Padding="5"
                                                                    BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                                    CornerRadius="15">
                                                                    <Grid>
                                                                        <StackLayout
                                                                            Margin="10,0,10,0"
                                                                            HeightRequest="45"
                                                                            HorizontalOptions="FillAndExpand"
                                                                            VerticalOptions="Center">
                                                                            <customItems:CustomEntry
                                                                                x:Name="EntryBodyTitle"
                                                                                FontSize="Small"
                                                                                MaxLength="256"
                                                                                Placeholder="Title"
                                                                                PlaceholderColor="{x:Static helpers:ThemeColors.TransparentTextColor}"
                                                                                TextColor="{x:Static helpers:ThemeColors.TextColor}" />
                                                                        </StackLayout>
                                                                        <Label
                                                                            Padding="0,0,10,0"
                                                                            FontSize="Caption"
                                                                            HorizontalOptions="EndAndExpand"
                                                                            TextColor="{Binding Source={x:Reference EntryBodyTitle}, Path=SizeLabelTextColor}"
                                                                            VerticalOptions="End">
                                                                            <Label.FormattedText>
                                                                                <FormattedString>
                                                                                    <Span Text="{Binding Source={x:Reference EntryBodyTitle}, Path=TextLenght}" />
                                                                                    <Span Text="/" />
                                                                                    <Span Text="{Binding Source={x:Reference EntryBodyTitle}, Path=MaxLength}" />
                                                                                </FormattedString>
                                                                            </Label.FormattedText>
                                                                        </Label>
                                                                    </Grid>
                                                                </Frame>
                                                                <Frame
                                                                    Padding="5"
                                                                    BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                                    CornerRadius="15">
                                                                    <Grid>
                                                                        <customItems:CustomEditor
                                                                            x:Name="entryBodyContent"
                                                                            Margin="10,5,5,25"
                                                                            FontSize="Small"
                                                                            HeightRequest="200"
                                                                            HorizontalOptions="FillAndExpand"
                                                                            MaxLength="2048"
                                                                            Placeholder="Description"
                                                                            PlaceholderColor="{x:Static helpers:ThemeColors.TransparentTextColor}"
                                                                            TextColor="{x:Static helpers:ThemeColors.TextColor}" />
                                                                        <Label
                                                                            Margin="5"
                                                                            Padding="0,0,10,0"
                                                                            FontSize="Caption"
                                                                            HorizontalOptions="EndAndExpand"
                                                                            TextColor="{Binding Source={x:Reference entryBodyContent}, Path=SizeLabelTextColor}"
                                                                            VerticalOptions="EndAndExpand">
                                                                            <Label.FormattedText>
                                                                                <FormattedString>
                                                                                    <Span Text="{Binding Source={x:Reference entryBodyContent}, Path=TextLenght}" />
                                                                                    <Span Text="/" />
                                                                                    <Span Text="{Binding Source={x:Reference entryBodyContent}, Path=MaxLength}" />
                                                                                </FormattedString>
                                                                            </Label.FormattedText>
                                                                        </Label>
                                                                    </Grid>
                                                                </Frame>
                                                                <Frame
                                                                    Padding="5"
                                                                    BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                                    CornerRadius="15">
                                                                    <StackLayout>
                                                                        <customItems:BorderlessEntry
                                                                            x:Name="entryBodyUrl"
                                                                            Margin="10,0,0,0"
                                                                            FontSize="Small"
                                                                            HorizontalOptions="FillAndExpand"
                                                                            Placeholder="URL"
                                                                            PlaceholderColor="{x:Static helpers:ThemeColors.TransparentTextColor}"
                                                                            TextColor="{x:Static helpers:ThemeColors.TextColor}" />
                                                                    </StackLayout>
                                                                </Frame>
                                                                <Frame
                                                                    Padding="5"
                                                                    BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                                    CornerRadius="15">
                                                                    <Grid>
                                                                        <customItems:CustomBoxView
                                                                            x:Name="embedBodyColorPicker"
                                                                            CornerRadius="255"
                                                                            HeightRequest="30"
                                                                            HorizontalOptions="FillAndExpand"
                                                                            VerticalOptions="Center"
                                                                            Color="#000000" />
                                                                        <Label
                                                                            FontAttributes="Bold"
                                                                            FontSize="Caption"
                                                                            HorizontalOptions="Center"
                                                                            Text="Embed Color"
                                                                            TextColor="{Binding Source={x:Reference embedBodyColorPicker}, Path=EmbedReverseColor}"
                                                                            VerticalOptions="Center" />
                                                                        <Grid.GestureRecognizers>
                                                                            <TapGestureRecognizer Tapped="embedBodyColorPicker_Tapped" />
                                                                        </Grid.GestureRecognizers>
                                                                    </Grid>
                                                                </Frame>
                                                            </StackLayout>
                                                        </customItems:CustomExpander.Content>
                                                    </customItems:CustomExpander>
                                                </Frame>
                                                <!--#endregion-->

                                                <!--#region Fields-->
                                                <Frame
                                                    Padding="5"
                                                    BackgroundColor="Transparent"
                                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                    CornerRadius="15">
                                                    <customItems:CustomExpander
                                                        x:Name="expEmbedFields"
                                                        AnimationLength="450"
                                                        CollapseAnimationEasing="SpringOut"
                                                        ExpandAnimationEasing="SpringIn"
                                                        HeaderText="Fields"
                                                        IsExpanded="False">
                                                        <customItems:CustomExpander.Content>
                                                            <StackLayout Margin="0,10">

                                                                <!--#region Field 1-->
                                                                <Frame
                                                                    Padding="5"
                                                                    BackgroundColor="Transparent"
                                                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                                    CornerRadius="15">
                                                                    <customItems:CustomExpander
                                                                        x:Name="expEmbedField1"
                                                                        AnimationLength="450"
                                                                        CollapseAnimationEasing="SpringOut"
                                                                        ExpandAnimationEasing="SpringIn"
                                                                        HeaderText="Field 1"
                                                                        IsExpanded="False">
                                                                        <customItems:CustomExpander.Content>
                                                                            <StackLayout Margin="0,10">
                                                                                <Frame
                                                                                    Padding="5"
                                                                                    BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                                                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                                                    CornerRadius="15">
                                                                                    <Grid>
                                                                                        <StackLayout
                                                                                            Margin="10,0,10,0"
                                                                                            HeightRequest="45"
                                                                                            HorizontalOptions="FillAndExpand"
                                                                                            VerticalOptions="Center">
                                                                                            <customItems:CustomEntry
                                                                                                x:Name="EntryFieldName1"
                                                                                                FontSize="Small"
                                                                                                MaxLength="256"
                                                                                                Placeholder="Name"
                                                                                                PlaceholderColor="{x:Static helpers:ThemeColors.TransparentTextColor}"
                                                                                                TextColor="{x:Static helpers:ThemeColors.TextColor}" />
                                                                                        </StackLayout>
                                                                                        <Label
                                                                                            Padding="0,0,10,0"
                                                                                            FontSize="Caption"
                                                                                            HorizontalOptions="EndAndExpand"
                                                                                            TextColor="{Binding Source={x:Reference EntryFieldName1}, Path=SizeLabelTextColor}"
                                                                                            VerticalOptions="End">
                                                                                            <Label.FormattedText>
                                                                                                <FormattedString>
                                                                                                    <Span Text="{Binding Source={x:Reference EntryFieldName1}, Path=TextLenght}" />
                                                                                                    <Span Text="/" />
                                                                                                    <Span Text="{Binding Source={x:Reference EntryFieldName1}, Path=MaxLength}" />
                                                                                                </FormattedString>
                                                                                            </Label.FormattedText>
                                                                                        </Label>
                                                                                    </Grid>
                                                                                </Frame>
                                                                                <Frame
                                                                                    Padding="5"
                                                                                    BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                                                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                                                    CornerRadius="15">
                                                                                    <Grid>
                                                                                        <customItems:CustomEditor
                                                                                            x:Name="entryFieldValue1"
                                                                                            Margin="10,5,5,25"
                                                                                            FontSize="Small"
                                                                                            HeightRequest="200"
                                                                                            HorizontalOptions="FillAndExpand"
                                                                                            MaxLength="1024"
