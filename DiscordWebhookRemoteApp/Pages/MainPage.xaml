                                                        AnimationLength="450"
                                                        CollapseAnimationEasing="SpringOut"
                                                        ExpandAnimationEasing="SpringIn"
                                                        HeaderText="Footer"
                                                        IsExpanded="False">
                                                        <customItems:CustomExpander.Content>
                                                            <StackLayout Margin="0,10">
                                                                <Frame
                                                                    Padding="5"
                                                                    BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                                    CornerRadius="15"
                                                                    HeightRequest="45">
                                                                    <StackLayout HeightRequest="45" Orientation="Horizontal">
                                                                        <Image
                                                                            x:Name="ImgEmbedFooterIconUrl"
                                                                            Margin="10,0,0,0"
                                                                            xct:CornerRadiusEffect.CornerRadius="360"
                                                                            HeightRequest="45"
                                                                            IsAnimationPlaying="True"
                                                                            Source="dcdemoimage.png"
                                                                            WidthRequest="45">
                                                                            <Image.GestureRecognizers>
                                                                                <TapGestureRecognizer Tapped="EmbedFooterAuthorImage_Tapped" />
                                                                            </Image.GestureRecognizers>
                                                                        </Image>
                                                                        <Grid HorizontalOptions="FillAndExpand">
                                                                            <customItems:CustomEntry
                                                                                x:Name="EntryFooterText"
                                                                                Margin="10,0,10,0"
                                                                                FontSize="Small"
                                                                                MaxLength="2048"
                                                                                Placeholder="Text"
                                                                                PlaceholderColor="{x:Static helpers:ThemeColors.TransparentTextColor}"
                                                                                TextColor="{x:Static helpers:ThemeColors.TextColor}" />
                                                                            <Label
                                                                                Padding="0,0,10,0"
                                                                                FontSize="Caption"
                                                                                HorizontalOptions="EndAndExpand"
                                                                                TextColor="{Binding Source={x:Reference EntryFooterText}, Path=SizeLabelTextColor}"
                                                                                VerticalOptions="End">
                                                                                <Label.FormattedText>
                                                                                    <FormattedString>
                                                                                        <Span Text="{Binding Source={x:Reference EntryFooterText}, Path=TextLenght}" />
                                                                                        <Span Text="/" />
                                                                                        <Span Text="{Binding Source={x:Reference EntryFooterText}, Path=MaxLength}" />
                                                                                    </FormattedString>
                                                                                </Label.FormattedText>
                                                                            </Label>
                                                                        </Grid>
                                                                    </StackLayout>
                                                                </Frame>
                                                                <Frame
                                                                    Padding="5"
                                                                    BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                                    CornerRadius="15">
                                                                    <StackLayout>
                                                                        <StackLayout Margin="10,0" Orientation="Horizontal">
                                                                            <customItems:CustomCheckBox
                                                                                x:Name="cbFooterSendInstantTime"
                                                                                HorizontalOptions="Start"
                                                                                IsCheckedChanged="cbFooterSendInstantTime_IsCheckedChanged"
                                                                                TitleText="Send instant time" />
                                                                            <Button
                                                                                x:Name="EmbedFooterSelectTimebtn"
                                                                                Margin="0,0,10,0"
                                                                                xct:TouchEffect.NativeAnimation="true"
                                                                                xct:TouchEffect.NativeAnimationColor="#2EFFCCA3"
                                                                                xct:TouchEffect.PressedAnimationDuration="500"
                                                                                xct:TouchEffect.PressedOpacity="0.6"
                                                                                xct:TouchEffect.PressedScale="0.98"
                                                                                BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                                                                BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                                                BorderWidth="1"
                                                                                Clicked="EmbedFooterSelectTimebtn_Clicked"
                                                                                CornerRadius="15"
                                                                                FontSize="Small"
                                                                                HeightRequest="45"
                                                                                HorizontalOptions="EndAndExpand"
                                                                                Text="Select Time"
                                                                                TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                                                                VerticalOptions="Center" />
                                                                        </StackLayout>
                                                                        <StackLayout
                                                                            Margin="0,5"
                                                                            HorizontalOptions="CenterAndExpand"
                                                                            Orientation="Horizontal">
                                                                            <customItems:CustomLabel
                                                                                x:Name="EmbedFooterTimestampSelectedLbl"
                                                                                FontSize="Caption"
                                                                                HorizontalOptions="Center"
                                                                                IsVisible="true"
                                                                                Text="Selected Date and Time"
                                                                                TextColor="{Binding Source={x:Reference EntryFooterText}, Path=SizeLabelTextColor}"
                                                                                VerticalOptions="Center" />
                                                                            <Label
                                                                                x:Name="EmbedFooterTimestampClearbtn"
                                                                                Margin="5,0,0,0"
                                                                                xct:TouchEffect.NativeAnimation="true"
                                                                                xct:TouchEffect.NativeAnimationColor="#2EFFCCA3"
                                                                                xct:TouchEffect.PressedAnimationDuration="500"
                                                                                xct:TouchEffect.PressedOpacity="0.6"
                                                                                xct:TouchEffect.PressedScale="0.98"
                                                                                BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                                                                FontSize="Small"
                                                                                HorizontalOptions="End"
                                                                                Text="Clear"
                                                                                TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                                                                VerticalOptions="Center">
                                                                                <Label.GestureRecognizers>
                                                                                    <TapGestureRecognizer Tapped="EmbedFooterTimestampClearbtn_Clicked" />
                                                                                </Label.GestureRecognizers>
                                                                            </Label>
                                                                        </StackLayout>
                                                                    </StackLayout>
                                                                </Frame>
                                                            </StackLayout>
                                                        </customItems:CustomExpander.Content>
                                                    </customItems:CustomExpander>
                                                </Frame>
                                                <!--#endregion-->

                                            </StackLayout>
                                        </customItems:CustomExpander.Content>
                                    </customItems:CustomExpander>
                                </Frame>
                            </StackLayout>
                        </Frame>
                        <!--#endregion-->

                        <!--#region Send File Area-->
                        <Frame
                            Padding="5"
                            BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                            BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                            CornerRadius="15"
                            IsVisible="True">
                            <StackLayout Margin="0,5">
                                <Label
                                    Margin="10,0,0,0"
                                    FontSize="Caption"
                                    HorizontalOptions="Start"
                                    Text="File"
                                    TextColor="{x:Static helpers:ThemeColors.TextColor}" />
                                <Frame
                                    Padding="5"
                                    BackgroundColor="Transparent"
                                    BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                    CornerRadius="15">
                                    <customItems:CustomExpander
                                        x:Name="expFileSend"
                                        AnimationLength="450"
                                        CollapseAnimationEasing="SpringOut"
                                        ExpandAnimationEasing="SpringIn"
                                        HeaderText="Send File">
                                        <customItems:CustomExpander.Content>
                                            <StackLayout Margin="0,10,0,5">
                                                <Grid HorizontalOptions="FillAndExpand">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="2*" />
                                                        <ColumnDefinition Width="*" />
                                                    </Grid.ColumnDefinitions>
                                                    <Label
                                                        Grid.Row="0"
                                                        Grid.RowSpan="2"
                                                        Grid.Column="0"
                                                        Margin="20,0,0,0"
                                                        FontSize="Small"
                                                        HorizontalOptions="FillAndExpand"
                                                        TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                                        VerticalOptions="Center">
                                                        <Label.FormattedText>
                                                            <FormattedString>
                                                                <Span Text="Selected: " />
                                                                <Span x:Name="lblSelectedFile" Text="none" />
                                                            </FormattedString>
                                                        </Label.FormattedText>
                                                    </Label>
                                                    <Button
                                                        x:Name="btnFileSelect"
                                                        Grid.Row="0"
                                                        Grid.Column="1"
                                                        Margin="5,0,10,0"
                                                        xct:TouchEffect.NativeAnimation="true"
                                                        xct:TouchEffect.NativeAnimationColor="#2EFFCCA3"
                                                        xct:TouchEffect.PressedAnimationDuration="500"
                                                        xct:TouchEffect.PressedOpacity="0.6"
                                                        xct:TouchEffect.PressedScale="0.98"
                                                        BackgroundColor="Transparent"
                                                        BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                        BorderWidth="1"
                                                        Clicked="btnFileSelect_Clicked"
                                                        CornerRadius="15"
                                                        HorizontalOptions="FillAndExpand"
                                                        Text="Select"
                                                        TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                                        VerticalOptions="Center" />
                                                    <Button
                                                        x:Name="btnFileClear"
                                                        Grid.Row="1"
                                                        Grid.Column="1"
                                                        Margin="5,0,10,0"
                                                        xct:TouchEffect.NativeAnimation="true"
                                                        xct:TouchEffect.NativeAnimationColor="#2EFFCCA3"
                                                        xct:TouchEffect.PressedAnimationDuration="500"
                                                        xct:TouchEffect.PressedOpacity="0.6"
                                                        xct:TouchEffect.PressedScale="0.98"
                                                        BackgroundColor="Transparent"
                                                        BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                                        BorderWidth="1"
                                                        Clicked="btnFileClear_Clicked"
                                                        CornerRadius="15"
                                                        HorizontalOptions="FillAndExpand"
                                                        Text="Clear"
                                                        TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                                        VerticalOptions="Center" />
                                                </Grid>
                                            </StackLayout>
                                        </customItems:CustomExpander.Content>
                                    </customItems:CustomExpander>
                                </Frame>
                            </StackLayout>
                        </Frame>
                        <!--#endregion-->

                        <!--#region Buttons-->
                        <StackLayout
                            HorizontalOptions="Center"
                            IsVisible="True"
                            Orientation="Horizontal">

                            <!--  Clear  -->
                            <Button
                                Margin="0,0,10,0"
                                xct:TouchEffect.NativeAnimation="true"
                                xct:TouchEffect.NativeAnimationColor="#2EFFCCA3"
                                xct:TouchEffect.PressedAnimationDuration="500"
                                xct:TouchEffect.PressedOpacity="0.6"
                                xct:TouchEffect.PressedScale="0.98"
                                BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                BorderWidth="1"
                                Clicked="clearContent_Clicked"
                                CornerRadius="15"
                                FontSize="Caption"
                                HeightRequest="45"
                                HorizontalOptions="Center"
                                Text="Clear"
                                TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                VerticalOptions="Center" />

                            <!--  Preview  -->
                            <Button
                                x:Name="btnPreview"
                                Margin="0,0,10,0"
                                xct:TouchEffect.NativeAnimation="true"
                                xct:TouchEffect.NativeAnimationColor="#2EFFCCA3"
                                xct:TouchEffect.PressedAnimationDuration="500"
                                xct:TouchEffect.PressedOpacity="0.6"
                                xct:TouchEffect.PressedScale="0.98"
                                BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                BorderWidth="1"
                                Clicked="Preview_Clicked"
                                CornerRadius="15"
                                FontSize="Caption"
                                HeightRequest="45"
                                HorizontalOptions="Center"
                                IsVisible="false"
                                Text="Preview"
                                TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                VerticalOptions="Center" />

                            <!--  SaveLoad  -->
                            <Button
                                x:Name="btnSaveLoadMessage"
                                Margin="0,0,10,0"
                                xct:TouchEffect.NativeAnimation="true"
                                xct:TouchEffect.NativeAnimationColor="#2EFFCCA3"
                                xct:TouchEffect.PressedAnimationDuration="500"
                                xct:TouchEffect.PressedOpacity="0.6"
                                xct:TouchEffect.PressedScale="0.98"
                                BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                BorderWidth="1"
                                Clicked="btnSaveLoadMessage_Clicked"
                                CornerRadius="15"
                                FontSize="Caption"
                                HeightRequest="45"
                                HorizontalOptions="Center"
                                IsVisible="false"
                                Text="Save &amp; Load"
                                TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                VerticalOptions="Center" />

                            <!--  Send  -->
                            <Button
                                Margin="0,0,10,0"
                                xct:TouchEffect.NativeAnimation="true"
                                xct:TouchEffect.NativeAnimationColor="#2EFFCCA3"
                                xct:TouchEffect.PressedAnimationDuration="500"
                                xct:TouchEffect.PressedOpacity="0.6"
                                xct:TouchEffect.PressedScale="0.98"
                                BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                                BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                                BorderWidth="1"
                                Clicked="sendContent_Clicked"
                                CornerRadius="15"
                                FontSize="Caption"
                                HeightRequest="45"
                                HorizontalOptions="Center"
                                Text="Send"
                                TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                VerticalOptions="Center" />
                        </StackLayout>
                        <!--#endregion-->

                        <StackLayout VerticalOptions="EndAndExpand">
                            <!--<Label
                            FontSize="Small"
                            HorizontalOptions="Center"
                            HorizontalTextAlignment="Center"
                            Text="Made By Shiroko💜"
                            TextColor="{x:Static helpers:ThemeColors.TextColor}"
                            VerticalOptions="EndAndExpand" />-->
                            <Label
                                Margin="0,0,0,2"
                                FontSize="Caption"
                                HorizontalOptions="Center"
                                HorizontalTextAlignment="Center"
                                Text="Designed By Zen💜"
                                TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                VerticalOptions="EndAndExpand" />
                        </StackLayout>
                    </StackLayout>
                </ScrollView>
                <Grid
                    x:Name="popupInfoBack"
                    BackgroundColor="#BA121212"
                    IsVisible="false"
                    VerticalOptions="FillAndExpand">
                    <Frame
                        Margin="30,0"
                        Padding="15,15,15,5"
                        BackgroundColor="{x:Static helpers:ThemeColors.BackColor}"
                        BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                        CornerRadius="15"
                        VerticalOptions="Center">
                        <StackLayout>
                            <Label
                                x:Name="lblPopupTitle"
                                Margin="5,0,0,0"
                                FontSize="Small"
                                HorizontalOptions="Start"
                                Text="Thanks for using our app."
                                TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                VerticalOptions="Start" />
                            <Label
                                x:Name="lblPopupContent"
                                Margin="5"
                                FontSize="Caption"
                                Text="Hey! If you think our apps are useful, you can support us by donating. ❤️"
                                TextColor="{x:Static helpers:ThemeColors.TextColor}" />
                            <StackLayout
                                Margin="0,5,0,0"
                                HorizontalOptions="Center"
                                Orientation="Horizontal">
                                <Button
                                    x:Name="btnOnayla"
                                    xct:TouchEffect.NativeAnimation="true"
                                    xct:TouchEffect.NativeAnimationColor="#2EFFCCA3"
                                    xct:TouchEffect.PressedAnimationDuration="500"
                                    xct:TouchEffect.PressedOpacity="0.6"
                                    xct:TouchEffect.PressedScale="0.98"
                                    BackgroundColor="Transparent"
                                    Clicked="btnSupport_Clicked"
                                    CornerRadius="15"
                                    FontSize="Caption"
                                    HorizontalOptions="Center"
                                    Text="Support"
                                    TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                    VerticalOptions="Center" />
                                <Button
                                    x:Name="btnIptal"
                                    xct:TouchEffect.NativeAnimation="true"
                                    xct:TouchEffect.NativeAnimationColor="#2EFFCCA3"
                                    xct:TouchEffect.PressedAnimationDuration="500"
                                    xct:TouchEffect.PressedOpacity="0.6"
                                    xct:TouchEffect.PressedScale="0.98"
                                    BackgroundColor="Transparent"
                                    Clicked="btnSupportCancel_Clicked"
                                    CornerRadius="15"
                                    FontSize="Caption"
                                    HorizontalOptions="Center"
                                    Text="Cancel"
                                    TextColor="{x:Static helpers:ThemeColors.TextColor}"
                                    VerticalOptions="Center" />
                            </StackLayout>
                        </StackLayout>
                    </Frame>
                </Grid>
                <Grid
                    x:Name="Loodinglayout"
                    BackgroundColor="Transparent"
                    IsVisible="false">
                    <Frame
                        BackgroundColor="Transparent"
                        CornerRadius="15"
                        HorizontalOptions="CenterAndExpand"
                        VerticalOptions="CenterAndExpand">
                        <StackLayout
                            Padding="50"
                            HorizontalOptions="Center"
                            VerticalOptions="Center">
                            <ActivityIndicator
                                HorizontalOptions="Center"
                                IsRunning="{Binding Source={x:Reference Loodinglayout}, Path=IsVisible}"
                                VerticalOptions="Center"
                                Color="{x:Static helpers:ThemeColors.TransparentTextColor}" />
                            <!--<Label
                            Margin="0,10,0,0"
                            FontSize="20"
                            Text="Yükleniyor..."
                            TextColor="White" />-->
                        </StackLayout>
                    </Frame>
                </Grid>
                <Grid IsVisible="{Binding Source={x:Reference MessageSaveLoadCV}, Path=IsVisible}" VerticalOptions="FillAndExpand">
                    <Grid
                        BackgroundColor="#BA121212"
                        IsVisible="{Binding Source={x:Reference MessageSaveLoadCV}, Path=IsVisible}"
                        VerticalOptions="FillAndExpand">
                        <Grid.GestureRecognizers>
                            <TapGestureRecognizer Tapped="MessageSaveLoadViewClose" />
                        </Grid.GestureRecognizers>
                    </Grid>
                    <Frame
                        Margin="30,10"
                        BackgroundColor="{x:Static helpers:ThemeColors.CardBackColor}"
                        BorderColor="{x:Static helpers:ThemeColors.BorderColor}"
                        CornerRadius="15">
                        <customViews:MessageSaveLoadView x:Name="MessageSaveLoadCV" IsVisible="false" />
                    </Frame>
                </Grid>
            </Grid>
            <StackLayout>
                <BoxView
                    Margin="0"
                    HeightRequest="1"
                    HorizontalOptions="Fill"
                    IsVisible="{Binding Source={x:Reference adsBottom}, Path=IsVisible}"
                    Color="{x:Static helpers:ThemeColors.BorderColor}" />
                <customItems:CustomAdsView
                    x:Name="adsBottom"
                    AdSize="Banner"
                    AdsId="ca-app-pub-3881259676793306/6797209579"
                    AutomationId="adsBottom"
                    VerticalOptions="End" />
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
